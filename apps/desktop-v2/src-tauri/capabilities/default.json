{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default-capability",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "webview:default",
    "app:default",
    "resources:default",
    "image:default",
    "menu:default",
    "tray:default",
    "store:default",
    "dialog:default",
    "shell:default",
    "upload:default",
    "notification:default",
    "log:default",
    "global-shortcut:default",
    "fs:default",
    "os:default"
  ]
}