{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "ext-shell",
    "description": "Enable all shell permissions.",
    "windows": [
        "*:shell*"
    ],
    "remote": {
        "urls": [
            "http://localhost:*",
            "https://*.huakun.tech*"
        ]
    },
    "permissions": [
        "shell:allow-open",
        "shell:allow-kill",
        "shell:allow-execute",
        "shell:allow-spawn",
        "shell:allow-stdin-write"
    ]
}